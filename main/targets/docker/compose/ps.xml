<?xml version="1.0" encoding="UTF-8"?>


<project default="docker:compose:ps">


    <property name="dockerComposePsDescription" value="show docker compose stack status" override="true"/>


    <target name="docker:compose:ps:init">
        <echo message="no properties defined"/>
    </target>


    <target name="docker:compose:ps:help" depends="docker:compose:ps:init">
        <echo message="${dockerComposePsDescription}"/>
    </target>


    <target name="docker:compose:ps" depends="docker:compose:ps:init">
        <exec command="docker-compose ps" passthru="true" checkreturn="true" level="info"/>
    </target>


</project>
