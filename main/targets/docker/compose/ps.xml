<?xml version="1.0" encoding="UTF-8"?>


<project default="docker:compose:ps">


    <property name="dockerComposePsDescription" value="show docker compose stack status" override="true"/>


    <target name="docker:compose:ps:help">
        <echo message="${dockerComposePsDescription}"/>
    </target>


    <target name="docker:compose:ps" depends="docker:compose:ps:before, docker:compose:ps:main, docker:compose:ps:after">
        <echo message="successful"/>
    </target>


    <target name="docker:compose:ps:before" hidden="true">
        <echo message="no actions defined"/>
    </target>


    <target name="docker:compose:ps:after" hidden="true">
        <echo message="no actions defined"/>
    </target>


    <target name="docker:compose:ps:main">
        <exec command="docker-compose ps" passthru="true" checkreturn="true" level="info"/>
    </target>


</project>
