<?xml version="1.0" encoding="UTF-8"?>


<project>


    <target name="project:help">
        <echo message="property                        value"/>
        <echo message="----------------------------------------------------------------------------------------------------"/>
        <echo message="project.name                    ${project.name}"/>
        <echo message="projectFolderMain             ${projectFolderMain}"/>
        <echo message="projectFolderMainComposer    ${projectFolderMainComposer}"/>
        <echo message="projectFolderMainCompass     ${projectFolderMainCompass}"/>
        <echo message="projectFolderMainNpm         ${projectFolderMainNpm}"/>
        <echo message="projectFolderBuild            ${projectFolderBuild}"/>
        <echo message=""/>
        <echo message="property                        value [do not edit directly]"/>
        <echo message="----------------------------------------------------------------------------------------------------"/>
        <echo message="projectVersion                 ${projectVersion}" level="warning"/>
        <echo message="projectRoot                    ${projectRoot}" level="warning"/>
        <echo message="project.dir.main                ${project.dir.main}" level="warning"/>
        <echo message="project.dir.main.composer       ${project.dir.main.composer}" level="warning"/>
        <echo message="project.dir.main.compass        ${project.dir.main.compass}" level="warning"/>
        <echo message="project.dir.main.npm            ${project.dir.main.npm}" level="warning"/>
        <echo message="project.dir.build               ${project.dir.build}" level="warning"/>
        <echo message="project.dir.build.tmp           ${project.dir.build.tmp}" level="warning"/>
        <echo message="project.dir.build.dist          ${project.dir.build.dist}" level="warning"/>
        <echo message="project.dir.build.reports       ${project.dir.build.reports}" level="warning"/>
    </target>


</project>
