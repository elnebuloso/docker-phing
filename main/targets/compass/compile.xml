<?xml version="1.0" encoding="UTF-8"?>


<project default="compass:compile">


    <property name="compass.compile.description" value="compass compile scss" override="true"/>


    <target name="compass:compile:help">
        <echo message="${compass.compile.description}"/>
    </target>


    <target name="compass:compile">
        <exec command="compass compile" passthru="true" checkreturn="true" level="info" dir="${project.dir.main.compass}"/>
    </target>


</project>