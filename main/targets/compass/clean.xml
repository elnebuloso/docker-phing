<?xml version="1.0" encoding="UTF-8"?>


<project default="compass:clean">


    <target name="compass:clean:init">
        <property name="compassCleanDescription" value="compass clean scss"/>
    </target>


    <target name="compass:clean:help" depends="compass:clean:init">
        <echo message="${compassCleanDescription}"/>
    </target>


    <target name="compass:clean" depends="compass:clean:init, compass:clean:before, compass:clean:main, compass:clean:after">
        <echo message="successful"/>
    </target>


    <target name="compass:clean:before" hidden="true">
        <echo message="no actions defined"/>
    </target>


    <target name="compass:clean:after" hidden="true">
        <echo message="no actions defined"/>
    </target>


    <target name="compass:clean:main">
        <exec command="compass clean" passthru="true" checkreturn="true" level="info" dir="${projectDirMainCompass}"/>
    </target>


</project>